package com.saucetest;

import java.net.MalformedURLException;
import java.net.URL;

import org.openqa.selenium.WebDriver;
import org.openqa.selenium.remote.DesiredCapabilities;
import org.openqa.selenium.remote.RemoteWebDriver;

public class SauceTest 
{
	public static final String USERNAME = "kundanprasad29@gmail.com";

	public static final String ACCESS_KEY = "xxxxx";

	public static final String URL = "http://" + USERNAME + ":" + ACCESS_KEY + "@ondemand.saucelabs.com:80/wd/hub";
	public static void main(String[] args) throws MalformedURLException
	{	
	DesiredCapabilities caps = DesiredCapabilities.chrome();
	
	caps.setCapability("platform", "Windows 10");

	caps.setCapability("version", "105");	
	
	WebDriver driver=new RemoteWebDriver(new URL(""), caps);

	driver.get("http://google.com");

	System.out.println(driver.getTitle());
	
	}


}
